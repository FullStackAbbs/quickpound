<%- include('header'); -%> // KEYWORD QUESTION
// refer to route app.get(post/:zebra)
//    routes.js (zebra is where gives us the specfic UNIQUE post picture ID)
// there now all the information for ONE particular post is now avaiable
  <ul>
  <% for(var i=0; i<posts.length; i++) {%>
    <li>
      <img src="../<%= posts[i].imgPath%>"> // takes in the imgPath to be seen
      <span><%= posts[i].caption %></span> // render the caption of that unique post
      <span><%= posts[i].likes %></span> // render and printed the likes of the uniwue post
      <span><i class="fa fa-heart" aria-hidden="true"></i></span>
      <span><i class="fa fa-trash" aria-hidden="true"></i></span>
    </li>
  <% } %>
  </ul>
  <a href="/profile">Return to Profile</a>
  <a href="/feed">Return to Feed</a>
<%- include('footer'); -%>
